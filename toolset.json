{
  "schemaVersion": "1.0",
  "swiftCompiler": {
    "extraCLIOptions": [
      "-Xfrontend",
      "-disable-stack-protector",
      "-enable-experimental-feature",
      "Embedded",
      "-enable-experimental-feature",
      "Extern",
      "-Xclang-linker",
      "-nostdlib",
      "-Xfrontend",
      "-function-sections",
      "-Xfrontend",
      "-mergeable-symbols",
      "-Xfrontend",
      "-enable-single-module-llvm-emission",
      "-assert-config",
      "Debug",
      "-Xclang-linker",
      "-fuse-ld=lld"
    ]
  },
  "cCompiler": {
    "extraCLIOptions": [
      "-nostdlib",
      "-ffunction-sections",
      "-mthumb",
      "-Werror",
      "-Wall",
      "-static",
      "-fno-stack-protector",
      "-fno-common"
    ]
  }
}
